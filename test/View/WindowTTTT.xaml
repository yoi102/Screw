<Window x:Class="test.View.WindowTTTT"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:test.View"
        mc:Ignorable="d"
        Title="WindowTTTT" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBox Text="{Binding TCom_Para2.Name}"/>

        <TextBox Grid.Row="1"  Text="{Binding TCom_Para2.Height}"/>

        <Button Grid.Row="2" Grid.Column="2" Command="{Binding SaveComand}"/>
    </Grid>
</Window>
